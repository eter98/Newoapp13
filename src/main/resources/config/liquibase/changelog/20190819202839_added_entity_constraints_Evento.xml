<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Evento.
    -->
    <changeSet id="20190819202839-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="categoria_evento_id"
                                 baseTableName="evento"
                                 constraintName="fk_evento_categoria_evento_id"
                                 referencedColumnNames="id"
                                 referencedTableName="categoria_contenidos"/>

        <addForeignKeyConstraint baseColumnNames="grupos_id"
                                 baseTableName="evento"
                                 constraintName="fk_evento_grupos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="grupos"/>

        <addForeignKeyConstraint baseColumnNames="miembro_id"
                                 baseTableName="evento"
                                 constraintName="fk_evento_miembro_id"
                                 referencedColumnNames="id"
                                 referencedTableName="miembros"/>

    </changeSet>
</databaseChangeLog>
