<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Feed.
    -->
    <changeSet id="20190820141152-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="miembro_id"
                                 baseTableName="feed"
                                 constraintName="fk_feed_miembro_id"
                                 referencedColumnNames="id"
                                 referencedTableName="miembros"/>

        <addForeignKeyConstraint baseColumnNames="categoria_feed_id"
                                 baseTableName="feed"
                                 constraintName="fk_feed_categoria_feed_id"
                                 referencedColumnNames="id"
                                 referencedTableName="categoria_contenidos"/>

    </changeSet>
</databaseChangeLog>
