<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity RegistroFacturacion.
    -->
    <changeSet id="20190819202823-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="tipo_registro_id"
                                 baseTableName="registro_facturacion"
                                 constraintName="fk_registro_facturacion_tipo_registro_id"
