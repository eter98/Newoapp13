<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity EntradaMiembros.
    -->
    <changeSet id="20190819194530-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="miembro_id"
                                 baseTableName="entrada_miembros"
                                 constraintName="fk_entrada_miembros_miembro_id"
                                 referencedColumnNames="id"
                                 referencedTableName="miembros"/>

        <addForeignKeyConstraint baseColumnNames="espacio_id"
                                 baseTableName="entrada_miembros"
                                 constraintName="fk_entrada_miembros_espacio_id"
                                 referencedColumnNames="id"
                                 referencedTableName="espacio_libre"/>

        <addForeignKeyConstraint baseColumnNames="oficina_id"
                                 baseTableName="entrada_miembros"
                                 constraintName="fk_entrada_miembros_oficina_id"
                                 referencedColumnNames="id"
                                 referencedTableName="landing"/>

        <addForeignKeyConstraint baseColumnNames="espacio_reserva_id"
                                 baseTableName="entrada_miembros"
                                 constraintName="fk_entrada_miembros_espacio_reserva_id"
                                 referencedColumnNames="id"
                                 referencedTableName="espacios_reserva"/>

    </changeSet>
</databaseChangeLog>
